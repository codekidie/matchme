{
  "expo": {
    "name": "Match Me",
    "description": "Sci Plant",
    "slug": "matchme",
    "githubUrl": "https://github.com/flatlogic/react-native-starter",
    "primaryColor": "#555CC4",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "assetBundlePatterns": ["assets/**/**"],
    "splash": {
      "image": "./assets/images/background.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.matchme.lite"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "android": {
      "package": "com.matchme.lite",
      "versionCode": 2
    }
  }
}
